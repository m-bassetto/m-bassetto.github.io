Cell[CellGroupData[{

Cell["First Order Conditions", "Section"],

Cell[BoxData[
    \(eq1[tauhigh_, tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \(u'\)[elow\  - \ tau1low]\  - \ mu\)], "Input"],

Cell[BoxData[
    \(eq3[tauhigh_, tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \(u'\)[ehigh - tauhigh] \((1 + lambda)\)\  - \ 
        mu\)], "Input"],

Cell[BoxData[
    \(eq4[tauhigh_, tau1low_, tau0low_, mu_, m_, lambda_]\  = \ 
      mbar\ \(u'\)[elow - tau0low]\  - \ 
        lambda\ \((1 - mbar)\) \(u'\)[ehigh - tau0low]\  - \ 
        mbar\ mu\)], "Input"],

Cell[BoxData[
    \(eq5[tauhigh_, tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \((1 - 
              m)\)\ tauhigh\  + \ \(\(m\)\(\ \)\((pi[
                  m]\ tau1low\  + \ \((1 - 
                    pi[m])\)\ tau0low)\)\(\ \)\)\)], "Input"],

Cell[BoxData[
    \(eq5lowlow[tauhigh_, tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \((1 - 
              m)\)\ tauhigh\  + \ \(\(m\)\(\ \)\(tau1low\)\(\ \ \)\)\)], \
"Input"]
}, Open  ]],

