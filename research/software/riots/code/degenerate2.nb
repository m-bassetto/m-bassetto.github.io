Cell["Optimal policy over [pibar;1]", "Subsection"],

Cell[BoxData[
    \(\(eqvec[tauhigh_, tau1low_, tau0low_, mu_, m_, 
          lambda_] = \[IndentingNewLine]{eq1[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq3[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq4[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq5[tauhigh, tau1low, tau0low, mu, 
            m, lambda]};\)\)], "Input"],

Cell[BoxData[
    \(policyvec[m_, lambda_, tauhighstart_, tau1lowstart_, tau0lowstart_, 
        mustart_] := \((policyrule = 
          FindRoot[
            eqvec[tauhigh, tau1low, tau0low, mu, m, 
              lambda], \[IndentingNewLine]{tauhigh, tauhighstart}, {tau1low, 
              tau1lowstart}, {tau0low, tau0lowstart}, {mu, 
              mustart}]; \[IndentingNewLine]{tauhigh, tau1low, tau0low, mu} /. 
          policyrule)\)\)], "Input"],

Cell[BoxData[
    \(policyvec[1,  .1,  .9,  .1,  .01, 1.5]\)], "Input"],

Cell[BoxData[
    \(Delta[m_, lambda_, tauhighstart_, tau1lowstart_, tau0lowstart_, 
        mustart_] := \((pt = 
          policyvec[m, lambda, tauhighstart, tau1lowstart, tau0lowstart, 
            mustart]; \[IndentingNewLine]tauhight = pt[\([1]\)]; 
        tau0lowt = 
          pt[\([3]\)]; \[IndentingNewLine]u[
            ehigh - tauhight] - \((1 - pi[m])\) 
            u[ehigh - tau0lowt])\)\)], "Input"],

Cell[BoxData[
    \(Delta[1,  .2,  .924,  .1,  .01, 1.58114]\)], "Input"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Optimal policy over [0;pibar]", "Subsection"],

Cell[BoxData[
