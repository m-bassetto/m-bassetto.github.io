            Delta[1, Sum[lambdagridmm[\([iim]\)], {iim, imlowmm, imxbarmm}], 
              tauhight0, tau1lowt0, tau0lowt0, mut0]; \[IndentingNewLine]For[
            im = imhighmm - 1, 
            im \[GreaterEqual] 
              imlowmm, \(im--\), \[IndentingNewLine]\((deltagridmm[\([im]\)] \
= Delta[mgridmm[\([im]\)], \[IndentingNewLine]Sum[
                    lambdagridmm[\([iim]\)] 
                      fpdf[1 - \((1 - mgridmm[\([im]\)])\)/\((1 - 
                                  mgridmm[\([iim]\)])\)]/
                        fpdf[mgridmm[\([im]\)]], {iim, imlowmm, 
                      Min[im, imxbarmm]}], \[IndentingNewLine]tauhight0, 
                  tau1lowt0, tau0lowt0, 
