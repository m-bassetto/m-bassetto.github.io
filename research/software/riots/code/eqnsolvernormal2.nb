Cell["Private Information: Optimal Policy", "Section"],

Cell[CellGroupData[{

Cell["Optimal policy", "Subsection"],

Cell[BoxData[
    \(\(Delta[m_, lambda_, tauhighstart_, tau1lowstart_, tau0lowstart_, 
          mustart_] := \[IndentingNewLine]If[
          m > pibar, \[IndentingNewLine]Deltamain[m, lambda, tauhighstart, 
            tau1lowstart, tau0lowstart, 
            mustart], \[IndentingNewLine]Deltalowlow[m, lambda, tauhighstart, 
            tau1lowstart, mustart]\[IndentingNewLine]];\)\)], "Input"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Optimal policy over (pibar;phigh]", "Subsection"],

Cell[BoxData[
    \(\(eqvec[tauhigh_, tau1low_, tau0low_, mu_, m_, 
          lambda_] = \[IndentingNewLine]{eq1[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq3[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq4[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq5[tauhigh, tau1low, tau0low, mu, 
            m, lambda]};\)\)], "Input"],

Cell[BoxData[
    \(\(policyrule[m_, lambda_, tauhighstart_, tau1lowstart_, tau0lowstart_, 
          mustart_] := \[IndentingNewLine]FindRoot[
          eqvec[tauhigh, tau1low, tau0low, mu, m, 
            lambda], \[IndentingNewLine]{tauhigh, tauhighstart}, {tau1low, 
            tau1lowstart}, {tau0low, tau0lowstart}, {mu, 
            mustart}];\)\)], "Input"],

Cell[BoxData[
    \(Deltamain[m_, lambda_, tauhighstart_, tau1lowstart_, tau0lowstart_, 
        mustart_] := \((pt = 
          policyrule[m, lambda, tauhighstart, tau1lowstart, tau0lowstart, 
            mustart]; \[IndentingNewLine]u[ehigh - tauhigh] - \((1 - pi[m])\) 
              u[ehigh - tau0low] /. pt)\)\)], "Input"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Optimal policy over [0;pibar]", "Subsection"],

Cell[BoxData[
    \(\(eqveclowlow[tauhigh_, tau1low_, tau0low_, mu_, m_, 
          lambda_]\  = \[IndentingNewLine]{eq1[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq3[tauhigh, tau1low, tau0low, mu, 
            m, lambda], \[IndentingNewLine]eq5lowlow[tauhigh, tau1low, 
            tau0low, mu, m, lambda]};\)\)], "Input"],

Cell[BoxData[
    \(\(policyrulelowlow[m_, lambda_, tauhighstart_, tau1lowstart_, 
          mustart_]\  := \[IndentingNewLine]FindRoot[
          eqveclowlow[tauhigh, tau1low, 0, mu, m, 
            lambda], \[IndentingNewLine]{tauhigh, tauhighstart}, {tau1low, 
            tau1lowstart}, {mu, mustart}];\)\)], "Input"],

Cell[BoxData[
    \(Deltalowlow[m_, lambda_, tauhighstart_, tau1lowstart_, 
        mustart_]\  := \ \((pt\  = \ 
          policyrulelowlow[m, lambda, tauhighstart, tau1lowstart, 
            mustart]; \[IndentingNewLine]pt = 
          Join[pt, {tau0low \[Rule] 0}]; \[IndentingNewLine]u[
            ehigh - tauhigh] /. pt)\)\)], "Input"]
}, Open  ]]
}, Open  ]],

