Cell[CellGroupData[{

Cell["First Order Conditions", "Section"],

Cell[BoxData[
    \(\(\( (*\ 
      first\ order\ condition\ with\ respect\ to\ tau1low\ *) \)\(\
\[IndentingNewLine]\)\(eq1[tauhigh_, tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \(u'\)[elow\  - \ tau1low]\  - \ mu\)\)\)], "Input"],

Cell[BoxData[
    \(\(\( (*\ 
      first\ order\ condition\ with\ respect\ to\ tauhigh\ *) \)\(\
\[IndentingNewLine]\)\(eq3[tauhigh_, tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \(u'\)[ehigh - tauhigh] \((1 + lambda)\)\  - \ 
        mu\)\)\)], "Input"],

Cell[BoxData[
    \(\(\( (*\ 
      first\ order\ condition\ with\ respect\ to\ tau0low\ over\ [plow; 
          phigh]\ *) \)\(\[IndentingNewLine]\)\(eq4[tauhigh_, tau1low_, 
        tau0low_, mu_, m_, lambda_]\  = \ 
      m\ \(u'\)[elow - tau0low]\  - \ 
        lambda\ \((1 - m)\) \(u'\)[ehigh - tau0low]\  - \ 
        m\ mu\)\)\)], "Input"],

Cell[BoxData[
    \(\(\( (*\ 
      resource\ constraint\ *) \)\(\[IndentingNewLine]\)\(eq5[tauhigh_, 
        tau1low_, tau0low_, mu_, m_, 
        lambda_]\  = \ \((1 - m)\)\ tauhigh\  + \ 
        m\ \((pi[m]\ tau1low\  + \ \((1 - pi[m])\)\ tau0low)\)\)\)\)], "Input"],

Cell[BoxData[
    \(\(\( (*\ 
      resource\ constraint\ over\ [0; 
          pibar]\ *) \)\(\[IndentingNewLine]\)\(eq5lowlow[tauhigh_, tau1low_, 
        tau0low_, mu_, m_, 
        lambda_]\  = \ \((1 - 
              m)\)\ tauhigh\  + \ \(\(m\)\(\ \)\(tau1low\)\(\ \ \)\)\)\)\)], \
"Input"]
}, Open  ]],

